<p id="notice"><%= notice %></p>

<h1>Empresas</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Historia</th>
      <th>Resumen</th>
      <th>Direccion</th>
      <th>Web</th>
      <th>Telefono</th>
      <th>Email</th>
      <th>Video</th>
      <th>Horario</th>
      <th>Lon</th>
      <th>Lat</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empresas.each do |empresa| %>
      <tr>
        <td><%= empresa.nombre %></td>
        <td><%= empresa.historia %></td>
        <td><%= empresa.resumen %></td>
        <td><%= empresa.direccion %></td>
        <td><%= empresa.web %></td>
        <td><%= empresa.telefono %></td>
        <td><%= empresa.email %></td>
        <td><%= empresa.video %></td>
        <td><%= empresa.horario %></td>
        <td><%= empresa.lon %></td>
        <td><%= empresa.lat %></td>
        <td><%= empresa.user_id %></td>
        <td><%= link_to 'Show', empresa %></td>
        <td><%= link_to 'Edit', edit_empresa_path(empresa) %></td>
        <td><%= link_to 'Destroy', empresa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empresa', new_empresa_path %>
